<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/01/nuspec.xsd">
    <metadata minClientVersion="2.5">
        <id>openssl-vc141</id>
        <title>openssl-vc141</title>
        <version>1.1.0.0</version>
        <authors>The OpenSSL Project</authors>
        <owners>kobake</owners>
        <licenseUrl>https://github.com/openssl/openssl/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/kobake/openssl-vc141-nuget</projectUrl>
        <iconUrl>https://raw.githubusercontent.com/kobake/openssl-vc141-nuget/master/logo.png</iconUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <summary>OpenSSL library packaged for Visual Studio 2017 (msvc141). This package is experimental, so use at your own risk.</summary>
        <description>OpenSSL is an open source project that provides a robust, commercial-grade, and full-featured toolkit for the Transport Layer Security (TLS) and Secure Sockets Layer (SSL) protocols.</description>
        <releaseNotes>OpenSSL 1.1.0e https://www.openssl.org/news/changelog.html</releaseNotes>
        <copyright>Copyright 2017</copyright>
        <tags>native, openssl, C, C++</tags>
        <dependencies>
        </dependencies>
    </metadata>
    <files>
        <file src="package.targets"                target="build\native\openssl-vc141.targets" />

        <file src="..\openssl\include\openssl\*.h" target="build\native\include\openssl\" />

        <file src="..\openssl\libssl.lib"          target="build\native\bin\" />
        <file src="..\openssl\libcrypto.lib"       target="build\native\bin\" />
        <file src="..\openssl\libssl-1_1.dll"      target="build\native\bin\" />
        <file src="..\openssl\libcrypto-1_1.dll"   target="build\native\bin\" />
    </files>
</package>
